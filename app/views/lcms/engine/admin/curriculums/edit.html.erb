<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/themes/default/style.min.css"/>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/jstree.min.js"></script>

<% set_page_title 'Curriculum Tree Manager' %>

<div class="o-page">

  <h1 class="text-center">Import Curriculum Tree</h1>

  <% if @curriculum.present? %>
    <h1 class="text-center">Curriculum Tree</h1>
    <div class="row">
      <%= react_component('admin/curriculum/CurriculumEditor', @curriculum.editor_props.merge(form_token: form_authenticity_token)) %>
    </div>
  <% end %>
</div>
